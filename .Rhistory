complete <- function(directory, id=1:322){
file_list <- list.files(directory, full.names=TRUE)
dat <- data.frame()
for (i in id)
{
dat <- rbind(dat, read.csv(file_list[i]))
dat_s <- dat["sulfate"]
na_s <- sum(dat_sul, na.rm=TRUE)
dat_n <- dat["nitrate"]
na_n <- sum(dat_nit, na.rm=TRUE)
if (na_n>na_s){
print (i)
print (na_s)
}
else {
print (i)
print (na_n)
}
}
}
